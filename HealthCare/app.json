{
  "expo": {
    "name": "HealthCare",
    "slug": "HealthCare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/androidIcon.png",
      "config": {
        "permissions": [
          "INTERNET"
        ],
        "package": "healthcare",
        "versionCode": 2,
        "supportsTablet": true,
        "cleartextTraffic": {
          "permitted": true
        },
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./assets/favicon.png"
      },
      "package": "com.alexandrevpacheco.HealthCare"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
